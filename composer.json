{
    "name": "symbiote/silverstripe-watching",
    "description": "Allows users to watch content items",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote",
        "watch",
        "subscribe"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^5",
        "symbiote/silverstripe-apiwrapper": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "cambis/silverstripe-rector": "^0.5.1",
        "phpunit/phpunit": "^9.5",
        "syntro/silverstripe-phpstan": "^5",
        "nswdpc/ci-files": "^1",
        "phpstan/phpstan": "~1.11.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/nswdpc/ci-files.git"
        },
        {
            "type": "git",
            "url": "https://github.com/JamesDPC/silverstripe-apiwrapper.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Symbiote\\Watch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symbiote\\Watch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
          "client/script"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
