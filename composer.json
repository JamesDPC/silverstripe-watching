{
    "name": "symbiote/silverstripe-watching",
    "description": "Allows users to watch content items",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote",
        "watch",
        "subscribe"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "~4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\Watch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symbiote\\Watch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
          "client/script"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
